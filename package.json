{
  "name": "chat-bot-ufc-ask",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "build": "npm run clean && npm run copy-assets",
    "clean": "rm -rf dist && mkdir dist",
    "copy-assets": "cp -r public dist/ && cp -r views dist/ && cp -r config dist/ && cp -r middleware dist/ && cp -r models dist/ && cp -r routers dist/ && cp -r services dist/ && cp index.js dist/ && cp package.json dist/",
    "prod": "cross-env NODE_ENV=production node index.js",
    "prod:dev": "cross-env NODE_ENV=production nodemon index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "install:prod": "npm ci --only=production",
    "install:dev": "npm install",
    "lint": "echo \"No linter configured\"",
    "lint:fix": "echo \"No linter configured\"",
    "check": "npm run lint && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.3",
    "selenium-webdriver": "^4.34.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "chromedriver": "^138.0.3",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.0",
    "rimraf": "^5.0.10"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "chromedriver_version": "138.0.7204.158"
  }
}
